{
  "name": "statpatternhub",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "cross-env TS_NODE_SKIP_IGNORE=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_COMPILER_OPTIONS='{\"skipLibCheck\":true}' dotenv -e .env.local -- npx vercel dev --listen 3001 --yes",
    "build": "vite build",
    "preview": "vite preview",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:verify": "dotenv -e .env.local -- tsx scripts/verify-migration.ts",
    "postinstall": "rm -rf node_modules/zod/v4 node_modules/zod/v4-mini && cd node_modules/zod && ln -s v3 v4 || true"
  },
  "dependencies": {
    "@ax-llm/ax": "^16.0.6",
    "@ax-llm/ax-ai-sdk-provider": "^16.0.7",
    "@clerk/backend": "^2.29.0",
    "@clerk/clerk-react": "^5.59.2",
    "@google/genai": "^1.34.0",
    "@tailwindcss/vite": "^4.1.18",
    "@upstash/redis": "^1.36.0",
    "@vercel/postgres": "^0.10.0",
    "ai": "5.0.118",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "es-module-lexer": "^1.7.0",
    "jszip": "^3.10.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.6.0",
    "svix": "^1.82.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.14.0",
    "@vercel/node": "^5.5.16",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "express": "^5.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vercel": "^47.0.0",
    "vite": "^6.2.0"
  },
  "overrides": {
    "typescript": "~5.8.2",
    "es-module-lexer": "^1.7.0"
  }
}
